{
  "assistant": {
    "name": "DALI",
    "wake_word_path": "wakeup_word/hello-dali_en_windows_v3_0_0.ppn",
    "language": "en-in",
    "voice_rate": 160,
    "session_timeout": 120,
    "listen_timeout": 10
  },
  "offline": {
    "vosk_models": {
      "en-in": "models/vosk-model-en-in-0.5",
      "hi-in": "models/vosk-model-hi-0.22"
    },
    "rasa_model": "models/rasa/models"
  },
  "online": {
    "timeout": 30,
    "use_openai": true,
    "openai_model": "gpt-3.5-turbo"
  },
  "voices": {
    "en-in": "Zira",     
    "hi-in": "Microsoft Hemant"
  },
  "keys": {
    "picovoice": "${PICOVOICE_ACCESS_KEY}",
    
    "openai_api_key": "${OPENAI_API_KEY}"
  }
}
